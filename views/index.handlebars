<div class="row" id="usernameContainer">
	<div class="col-md-4 col-md-offset-4 welcomeText text-center">
		<h1>{{welcomeText}}</h1>
	</div>
</div>
<br>
<div class="row">
	<div class="col-md-4 col-md-offset-4 authInputs">
		<input id="username" placeholder="Username" />
	</div>
</div>
<br>
<div class="row">
	<div class="col-md-4 col-md-offset-4 authInputs">
		<input id="password" type="password" placeholder="Password" />
	</div>
</div>
<br>
<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<a class="btn btn-primary pull-right" id="submitUser" data-type="{{actionBtn}}">Submit</a>
	</div>
</div>